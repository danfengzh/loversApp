<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.loversAPP.dao.SheduleTestDao">

    <insert id="insert">
        INSERT SheduleTest(currentDate,content) VALUES(#{sheduleTest.currentDate},#{sheduleTest.content})
    </insert>
    <update id="updateSheduleTestByID">
        UPDATE SheduleTest
        SET content=#{content}
        where id=#{id}
    </update>
</mapper>